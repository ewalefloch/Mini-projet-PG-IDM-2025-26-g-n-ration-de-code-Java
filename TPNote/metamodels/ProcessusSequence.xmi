<?xml version="1.0" encoding="UTF-8"?>
<LDP2:Processus
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:LDP2="http://LDPSequence/1.0">
  <elements
      xsi:type="LDP2:Activite"
      nom="Init"
      suivantes="//@elements.1">
    <actions
        xsi:type="LDP2:InstanceObj"
        nom="calc"
        classe="Calcul"/>
    <actions
        xsi:type="LDP2:CreationVar"
        nom="valeur1"
        type="int"
        valeur="5"/>
    <actions
        xsi:type="LDP2:CreationVar"
        nom="valeur2"
        type="int"
        valeur="10"/>
    <actions
        xsi:type="LDP2:CreationVar"
        nom="resA"
        type="int"
        valeur="0"/>
    <actions
        xsi:type="LDP2:CreationVar"
        nom="resB"
        type="int"
        valeur="0"/>
  </elements>
  <elements
      xsi:type="LDP2:Fourche"
      nom="Separation"
      suivantes="//@elements.2 //@elements.3"
      precedents="//@elements.0"/>
  <elements
      xsi:type="LDP2:Activite"
      nom="BrancheFactorielle"
      suivantes="//@elements.4"
      precedents="//@elements.1">
    <actions
        xsi:type="LDP2:AppelMethode"
        resultat="resA"
        nomMethode="factorial"
        objetAppele="calc">
      <parametres>valeur1</parametres>
    </actions>
  </elements>
  <elements
      xsi:type="LDP2:Activite"
      nom="BranchePuissance"
      suivantes="//@elements.4"
      precedents="//@elements.1">
    <actions
        xsi:type="LDP2:AppelMethode"
        resultat="resB"
        nomMethode="power"
        objetAppele="calc">
      <parametres>valeur1</parametres>
      <parametres>valeur2</parametres>
    </actions>
  </elements>
  <elements
      xsi:type="LDP2:Jonction"
      nom="Synchronisation"
      suivantes="//@elements.5"
      precedents="//@elements.2 //@elements.3"/>
  <elements
      xsi:type="LDP2:Activite"
      nom="Finalisation"
      precedents="//@elements.4">
    <actions
        xsi:type="LDP2:InstanceObj"
        nom="log"
        classe="Logger"/>
    <actions
        xsi:type="LDP2:AppelMethode"
        nomMethode="printValue"
        objetAppele="log">
      <parametres>resA</parametres>
    </actions>
    <actions
        xsi:type="LDP2:AppelMethode"
        nomMethode="printValue"
        objetAppele="log">
      <parametres>resB</parametres>
    </actions>
  </elements>
  <debut reference="//@elements.0"/>
  <fin reference="//@elements.5"/>
</LDP2:Processus>
