<?xml version="1.0" encoding="UTF-8"?>
<LDP2:Processus
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:LDP2="http://LDPSequence/1.0">

  <elements xsi:type="LDP2:Activite" nom="Init" suivantes="//@elements.1">
    
    <actions xsi:type="LDP2:InstanceObj">
      <objet name="calc" type="Calcul">
        <methods nomMethode="factorial"/> 
        <methods nomMethode="power"/>
      </objet>
    </actions>

    <actions xsi:type="LDP2:CreationVar">
      <var name="valeur1" type="int" valeur="5"/>
    </actions>

    <actions xsi:type="LDP2:CreationVar">
      <var name="valeur2" type="int" valeur="10"/>
    </actions>

    <actions xsi:type="LDP2:CreationVar">
      <var name="resA" type="int" valeur="0"/>
    </actions>

    <actions xsi:type="LDP2:CreationVar">
      <var name="resB" type="int" valeur="0"/>
    </actions>
  </elements>

  <elements xsi:type="LDP2:Fourche" 
      nom="Separation" 
      suivantes="//@elements.2 //@elements.3" 
      precedents="//@elements.0"/>

  <elements xsi:type="LDP2:Activite" 
      nom="BrancheFactorielle" 
      suivantes="//@elements.4" 
      precedents="//@elements.1">
      
    <actions xsi:type="LDP2:AppelMethode" 
        resultat="//@elements.0/@actions.3/@var"
        objetAppele="//@elements.0/@actions.0/@objet"
        method="//@elements.0/@actions.0/@objet/@methods.0">
      <parametre parameters="//@elements.0/@actions.1/@var"/>
    </actions>
  </elements>

  <elements xsi:type="LDP2:Activite" 
      nom="BranchePuissance" 
      suivantes="//@elements.4" 
      precedents="//@elements.1">
      
    <actions xsi:type="LDP2:AppelMethode" 
        resultat="//@elements.0/@actions.4/@var"
        objetAppele="//@elements.0/@actions.0/@objet"
        method="//@elements.0/@actions.0/@objet/@methods.1">
      <parametre parameters="//@elements.0/@actions.1/@var //@elements.0/@actions.2/@var"/>
    </actions>
  </elements>

  <elements xsi:type="LDP2:Jonction" 
      nom="Synchronisation" 
      suivantes="//@elements.5" 
      precedents="//@elements.2 //@elements.3"/>

  <elements xsi:type="LDP2:Activite" 
      nom="Finalisation" 
      precedents="//@elements.4">
      
    <actions xsi:type="LDP2:InstanceObj">
      <objet name="log" type="Logger">
         <methods nomMethode="printValue"/>
      </objet>
    </actions>

    <actions xsi:type="LDP2:AppelMethode" 
        objetAppele="//@elements.5/@actions.0/@objet"
        method="//@elements.5/@actions.0/@objet/@methods.0">
      <parametre parameters="//@elements.0/@actions.3/@var"/>
    </actions>

    <actions xsi:type="LDP2:AppelMethode" 
        objetAppele="//@elements.5/@actions.0/@objet"
        method="//@elements.5/@actions.0/@objet/@methods.0">
      <parametre parameters="//@elements.0/@actions.4/@var"/>
    </actions>
  </elements>

  <debut reference="//@elements.0"/>
  <fin reference="//@elements.5"/>
</LDP2:Processus>